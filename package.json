    "@types/react-dom": "^17.0.10",
    "@vitejs/plugin-react": "^2.0.0",
    "@testing-library/react": "^13.3.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4",
    "vite": "^3.0.0",
    "vitest": "^0.19.1"
  }
